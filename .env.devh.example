# Dev‑Hardened example env. Copy to .env.devh and customise.

# HS256 secret for Gateway JWT validation (local only)
DEVH_JWT_SECRET=devh-secret

# Admin token settings to generate a JWT (see scripts/gen_devh_jwt.py)
DEVH_ADMIN_SUB=devh-admin
DEVH_TENANT=devh-tenant

# Populated by scripts/gen_devh_jwt.py
DEVH_ADMIN_JWT=

# Internal token for worker→Gateway credentials fetch
GATEWAY_INTERNAL_TOKEN=dev-internal-token

# Fernet key for encrypting stored LLM credentials in Redis (urlsafe base64)
GATEWAY_ENC_KEY=O6qM9Oe7zB3w6CqQFctciVwEciXxV9nOcDSBxPTsPOg=

# UI port and Gateway port (bound to localhost)
AGENT_UI_PORT=20015
GATEWAY_PORT=20016

