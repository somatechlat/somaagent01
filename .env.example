# Agent Zero example environment file for local development.
# Copy this to agent-zero/.env and fill in secret values. Do NOT commit .env.

# Ports and service ports
AGENT_UI_PORT=21015
GATEWAY_PORT=21016
KAFKA_PORT=21000
REDIS_PORT=21001
POSTGRES_PORT=21002
OPA_PORT=21009

# Feature flags
USE_LLM=true
ANONYMIZED_TELEMETRY=false

# Authentication (set if you want UI auth)
AUTH_LOGIN=admin
AUTH_PASSWORD=

# Provider API keys (placeholders)
GROQ_API_KEY=
OPENROUTER_API_KEY=
OPENAI_API_KEY=
HUGGINGFACE_API_KEY=
GITHUB_COPILOT=

# LiteLLM global kwargs (as newline-separated key=value entries, when used)
LITELLM_GLOBAL_KWARGS=

# Example provider default model (used by settings defaults)
DEFAULT_CHAT_MODEL=openai/gpt-4.1
